{
  "name": "ent-esiex-backend",
  "version": "1.0.0",
  "description": "node-express-typescript-boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "update-sectors": "ts-node src/scripts/update-sectors.ts",
    "create-quiz": "ts-node scripts/create-quiz.ts",
    "import-stock-data": "ts-node scripts/importStockData.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:ts": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "ts-node src/seed-learning.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/express": "^5.0.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "prisma": "^6.17.1",
    "sanitize-html": "^2.17.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "ts-node": "^10.9.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/nodemailer": "^7.0.3",
    "@types/sanitize-html": "^2.16.0",
    "@types/xlsx": "^0.0.35",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
